<div class="panel panel-default login col-md-5 col-md-offset-3">
  	<div class="panel-body">
	  	<div class="row">
  			<p class="lead">&nbsp;&nbsp; Log In</p>
	  	</div>
	  	<div class="row">&nbsp;</div>
	  	<%= form_tag sessions_path do %>
    		<div class="row">
    			<div class="col-md-6">
    				Email:<br><%= text_field_tag :email, params[:email], :class => 'form-control' %>
   				</div>
   				<div class="col-md-3 col-md-offset-2">
   					<%= submit_tag '&nbsp;&nbsp; LOG IN  &nbsp;&nbsp;'.html_safe, :class => 'btn btn-default' %>
   				</div>				
   			</div>
			<div class="row">&nbsp;</div>
			<div class="row">
				<div class="col-md-6">
    				Password:<br><%= password_field_tag :password, params[:password], :class => 'form-control'  %>
				</div>
				<div class="col-md-4 col-md-offset-2">
					<%= check_box_tag :remember_me, 1, params[:remember_me] %> Remember me
				</div>
			</div>
		<% end %>
		<p>&nbsp;</p>
	</div>
</div>
